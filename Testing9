-- Customize Features

-- Hub Emoji
Emoji = getgenv().Hub_Emoji
-- if they don't put any thing for Hub Emoji
if getgenv().Hub_Emoji == nil then
    Emoji = "🔥"
end

-- Notification Emoji
getgenv().Emoji = getgenv().Notifi_Emoji
-- If they don't put any thing for Notification Emoji
if getgenv().Notifi_Emoji == nil then
    getgenv().Emoji = "✅"
end
