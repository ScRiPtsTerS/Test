    local Halloween = Window:NewTab("Halloween")
    local MainHalloween = Halloween:NewSection("Halloween Main")
    
    MainHalloween:NewToggle("Auto Collect Pumpkin", "collects Pumpkins for event", function (state)
        if state then
            getgenv().RebirthXConfigs.AutoCollectPumpkin = true;
            while getgenv().RebirthXConfigs.AutoCollectPumpkin == true do
                wait(0.1)
                local PumpkinCollect = game.Workspace.Scripts.PumpkinsCollect.Storage
                for i, v in pairs(PumpkinCollect:GetDescendants()) do
                   if v.Name == "PumpkinCollectModel" then
                        v.main.Position = game.Workspace.kirito_darkrepulsers.HumanoidRootPart.Position
                   end
                end
            end
        else
            getgenv().RebirthXConfigs.AutoCollectPumpkin = false;
        end
    end)

    MainHalloween:NewLabel("Set pumpkin number before pressing it")
    if getgenv().RebirthXConfigs.PumpkinNumber == "You Claimed all Pumpkins" then
