    MainHalloween:NewButton("Teleport to Pumpkins", "helps with Pumpkin hunt", function()
        local teleport_table = {Pumpkin1 = Vector3.new(119.121, -37.3428, -532.764),Pumpkin2 = Vector3.new(943.875, -38.5711, 748.5),Pumpkin3 = Vector3.new(295.647, 52.9395, -27.8584),Pumpkin4 = Vector3.new(467.674, 22.2271, -341.51),Pumpkin5 = Vector3.new(309.224, -54.9334, 151.089),Pumpkin6 = Vector3.new(362.442, 65.8116, 702.954),Pumpkin7 = Vector3.new(149.619, 48.5213, -509.631),Pumpkin8 = Vector3.new(-557.98, 32.0198, 597.958),Pumpkin9 = Vector3.new(-631.577, 3.05852, 1126.41)}
        local TS = game:GetService('TweenService')
        local TI = TweenInfo.new(1,Enum.EasingStyle.Linear)
        local LP = game.Players.LocalPlayer
        function teleport(v)
          if LP.Character and LP.Character:FindFirstChild('HumanoidRootPart') then
            local cf = CFrame.new(v)
            local a = TS:Create(LP.Character.HumanoidRootPart,TI,{CFrame=cf})
            a:Play()
            end
        end
        if getgenv().RebirthXConfigs.PumpkinNumber == "Pumpkin1" then
            teleport(teleport_table.Pumpkin1)
            dropdown:Refresh(newList1)
        elseif getgenv().RebirthXConfigs.PumpkinNumber == "Pumpkin2" then
            teleport(teleport_table.Pumpkin2)
            dropdown:Refresh(newList2)
        elseif getgenv().RebirthXConfigs.PumpkinNumber == "Pumpkin3" then
            teleport(teleport_table.Pumpkin3)
            dropdown:Refresh(newList3)
        elseif getgenv().RebirthXConfigs.PumpkinNumber == "Pumpkin4" then
            teleport(teleport_table.Pumpkin4)
            dropdown:Refresh(newList4)
        elseif getgenv().RebirthXConfigs.PumpkinNumber == "Pumpkin5" then
            teleport(teleport_table.Pumpkin5)
            dropdown:Refresh(newList5)
        elseif getgenv().RebirthXConfigs.PumpkinNumber == "Pumpkin6" then
            teleport(teleport_table.Pumpkin6)
            dropdown:Refresh(newList6)
        elseif getgenv().RebirthXConfigs.PumpkinNumber == "Pumpkin7" then
            teleport(teleport_table.Pumpkin7)
            dropdown:Refresh(newList7)
        elseif getgenv().RebirthXConfigs.PumpkinNumber == "Pumpkin8" then
            teleport(teleport_table.Pumpkin8)
            dropdown:Refresh(newList8)
        elseif getgenv().RebirthXConfigs.PumpkinNumber == "You Claimed all Pumpkins" then
            teleport(teleport_table.Pumpkin9)
        end
    end)

