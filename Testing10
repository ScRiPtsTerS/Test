    -- Saves the settings in a file
    function SaveData()
        Notification.new("info", "[Settings]:", "Saving Config "..notifiEmoji.."...", true, 1.5); -- Notifi Saving Data
        local json; -- json is required to save the data
        local HttpService = game:GetService("HttpService"); -- roblox HttpService
        if (writefile and makefolder) then -- if the exploit has this function
            json = HttpService:JSONEncode(getgenv().RebirthXConfigs); -- encodes all the settings to JSON
            makefolder(FolderName)
            writefile(filename, json); -- the file then whats in the setting
            Notification.new("success", "[Settings]:", "Config is saved! "..notifiEmoji, true, 1.5); -- Notifi Saved Data complete
        else -- else if the Exploit doesn't have the function
            Notification.new("error", "[Settings]:", "Can't save Settings: Bad Exploit or Try again "..notifiEmoji, true, 1.5); -- Exploit not support
        end
    end
    -- loads saved Settings
    function LoadData()
        Notification.new("success", "[Settings]:", "loading saved settings "..notifiEmoji, true, 1.5); -- Notifi loading data
        local HttpService = game:GetService("HttpService") -- roblox HttpService
        if (readfile and isfile and isfile(filename)) then
            getgenv().RebirthXConfigs = HttpService:JSONDecode(readfile(filename));
        else -- else if the settings can't be loaded
            Notification.new("error", "[Settings]:", "Can't load settings: bad exploit or file was not created "..notifiEmoji, true,1.5); -- Notifi error message
        end
    end

    wait(1)
    LoadData()
    
    MainRebirthX:NewToggle("Auto Tap", "Taps for you", function(state)
        if state then
        getgenv().RebirthXConfigs.AT = true; 
        SaveData()
    while getgenv().RebirthXConfigs.AT == true do
    game:GetService("ReplicatedStorage").Events.Click3:FireServer()
        wait()
    end
        else
            getgenv().RebirthXConfigs.AT = false;
            SaveData()
        end
    end)

