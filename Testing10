-- Functions
function GameSup() -- Game Supported
    getgenv().Supported = true
end
function Destroy() -- Destroy Gui
    game:GetService("CoreGui").infinfinf:Destroy() -- infinfinf = name of gui
end
function FPSBoost()
    local FPSBoost = Window:NewTab("FPS boost")
    local MainFPSBoost = FPSBoost:NewSection("Main")
    MainFPSBoost:NewButton("FPS boost 1", "makes you not lag alot", function()
    loadstring(game:HttpGet("pastebin.com/raw/ywFjby1i", true))()
    end)

    MainFPSBoost:NewLabel("FPS boost 2 with settings")
    MainFPSBoost:NewLabel("Set settings before Using")
    MainFPSBoost:NewButton("FPS boost 2", "makes you not lag alot", function()
        getgenv().Settings = {Players = {["Ignore Me"] = false,["Ignore Others"] = false},Meshes = {Destroy = false,LowDetail = false},Images = {Invisible = false,LowDetail = false,Destroy = false,},["No Particles"] = true,["No Camera Effects"] = true,["No Explosions"] = true,["No Clothes"] = true,["Low Water Graphics"] = true,["No Shadows"] = true,["Low Rendering"] = true,["Low Quality Parts"] = true}
        loadstring(game:HttpGet("https://pastebin.com/raw/cr6y1C60", true))()
    end)
MainFPSBoost:NewToggle("Ignore Players", "Ignores other players", function(state)
    if state then 
        getgenv().Settings.Players["Ignore Others"] = true
    else
        getgenv().Settings.Players["Ignore Others"] = false
    end
end)
MainFPSBoost:NewToggle("Ignore YourSelf", "Ignores other players", function(state)
    if state then 
        getgenv().Settings.Players["Ignore Me"] = true
    else
        getgenv().Settings.Players["Ignore Me"] = false
    end
end)
MainFPSBoost:NewToggle("Destroys meshes", "self Explains", function(state)
    if state then 
        getgenv().Settings.Meshes.Destroy = true
    else
        getgenv().Settings.Meshes.Destroy = false
    end
end)
MainFPSBoost:NewToggle("Low detail meshes", "self Explains", function(state)
    if state then 
        getgenv().Settings.Meshes.LowDetail = true
    else
        getgenv().Settings.Meshes.LowDetail = false
    end
end)
MainFPSBoost:NewToggle("Invisible Images", "self Explains", function(state)
    if state then 
        getgenv().Settings.Images.Invisible = true
    else
        getgenv().Settings.Images.Invisible = false
    end
end)
MainFPSBoost:NewToggle("LowDetail Images", "self Explains", function(state)
    if state then 
        getgenv().Settings.Images.LowDetail = true
    else
        getgenv().Settings.Images.LowDetail = false
    end
end)
MainFPSBoost:NewToggle("Destroy Images", "self Explains", function(state)
    if state then 
        getgenv().Settings.Images.Destroy = true
    else
        getgenv().Settings.Images.Destroy = false
    end
end)
end
function SupportedGames() -- Supported Games Tab
    local SupportedGames = Window:NewTab("Supported Games!!!")
    local MainSupportedGames = SupportedGames:NewSection("Games")
    MainSupportedGames:NewLabel("BedWars")
    MainSupportedGames:NewLabel("FlagWars")
    MainSupportedGames:NewLabel("Rebirth Champions X")
    MainSupportedGames:NewLabel("Chinese freebie game")
    MainSupportedGames:NewLabel("Tai Verdes Concert")
    MainSupportedGames:NewLabel("Islands of Move")
    MainSupportedGames:NewLabel("Mansion of Wonder")
    MainSupportedGames:NewLabel("24kGoldn Concert")
    MainSupportedGames:NewLabel("Race Clicker")
    MainSupportedGames:NewLabel("Big Paintball")
    MainSupportedGames:NewLabel("Arsenal")
    MainSupportedGames:NewLabel("type /console")
end
function ToggleGui() -- Toggle Gui
    -- Toggle Gui Window
    local Toggle = Window:NewTab("Toggle UI")
    local MainToggle = Toggle:NewSection("You can change settings")
    -- Toggle Gui Button
    MainToggle:NewKeybind("Toggle UI", "Self Explains", Enum.KeyCode.K, function()
        Library:ToggleUI()
        Notification.new("info", "Toggled Gui", "You Just Toggled the Gui "..notifiEmoji, true, 3)
    end)
end
function JoinDiscord()
    local Discord = Window:NewTab("Discord") -- Discord Button
    local MainDiscord = Discord:NewSection("Discord link")
    MainDiscord:NewButton("Copy link", "copys Dicord server link", function ()
        Notification.new("info", "Discord", "Dicord server is copied to clipboard! "..notifiEmoji, true, 3)
        setclipboard("https://discord.io/GigaSploits")
    end)
end
