    while getgenv().RebirthXConfigs.BWBoost == true do
    local args = {[1] = "Pixel"}
    game:GetService("ReplicatedStorage").Events.WorldBoost:FireServer(unpack(args))
        wait()   
    end
        else
            getgenv().RebirthXConfigs.BWBoost = false;
            SaveData()
        end    
    end)

